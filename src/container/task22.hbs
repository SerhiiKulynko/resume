<main>
    <div>        
        {{#with page_header}}
            <p class="h1">{{title}}</p><br>
            <p class="text-uppercase">{{description}}</p>
        {{/with}}    
    </div>
    <br>
    <div>        
        {{#with section}}
            <div class="card">
                <div class="card-body">
                {{#with about_us}}
                    <div>
                        {{#with title_list}}
                            <div class="display-6 text-end">{{main}}</div><br>  
                            <div class="h2 text-end">{{sub}}</div>
                        {{/with}}
                    </div>
                    <br>
                    <div>
                        {{#with button}}
                            <button type="button" class="btn btn-outline-info">{{text}}</button>
                        {{/with}}
                    </div>
                {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with advantage}}
                        <div class="display-5">{{title}}</div>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with efficiency}}
                                    <div class="display-4">{{title}}</div><br>
                                    <div class="h6">{{sub_title}}</div><br>
                                    <p class="text-start">{{description}}</p><br>
                                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                            </div>
                        </section>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with innovation}}
                                    <div class="display-4 text-center">{{title}}</div><br>
                                    <div class="h6 text-center">{{sub_title}}</div><br>
                                    <p class="text-center">{{description}}</p><br>
                                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                            </div>
                        </section>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with collaboration}}
                                    <div class="display-4 text-end">{{title}}</div><br>
                                    <div class="h6 text-end">{{sub_title}}</div><br>
                                    <p class="text-end">{{description}}</p><br>
                                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                            </div>
                        </section>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with agreement}}
                        <div class="display-5">{{title}}</div>
                        <br>
                        <div>
                            {{#with info}}
                                <div class="h5">{{sub_title}}</div>
                                <br>
                                <div class="card">
                                    <div class="card-body">
                                        {{#with detail}}
                                        <p>{{text}}</p>
                                        <button type="button" class="btn btn-success">{{button}}</button>
                                    {{/with}}
                                    </div>                                
                                </div>
                            {{/with}}
                        </div>
                    {{/with}}
                </div>
            </div>
        {{/with}}        
    </div>
</main>