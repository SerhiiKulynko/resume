<Main>    
    <div class="card"> 
        <div class="card-body">    
            {{#with heading}}            
                <div class="card">
                    <div class="card-body">
                        {{#with main}}
                            <h1>{{title}}</h1><br>
                            <p class="text-start">{{text}}</p><br>
                            <div>
                                {{#with button_text}}         
                                    <button type="button" class="btn btn-primary">{{solid}}</button><br><br>
                                    <button type="button" class="btn btn-outline-primary">{{outline}}</button>                            
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with second}}
                            <h2>{{title}}</h2>
                            <p class="text-start">{{text}}</p>
                            <div>
                                {{#with button_text}}
                                    <button type="button" class="btn btn-secondary">{{solid}}</button><br><br>
                                    <button type="button" class="btn btn-outline-secondary">{{outline}}</button>
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                </div>             
                <br>            
                <div class="card">
                    <div class="card-body">
                        {{#with subblock}}
                            <h3>{{title}}</h3>
                            <p class="text-start">{{text}}</p>
                            <div>
                                {{#with button_text}}
                                    <button type="button" class="btn btn-dark">{{solid}}</button><br><br>
                                    <button type="button" class="btn btn-outline-dark">{{outline}}</button>
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                </div>            
            {{/with}}
        </div>   
    </div>
    <br>    
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
                <div class="display-1">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with children_display}}
                            <div class="display-4">{{title}}</div>
                            <p class="text-start">{{text}}</p>
                            <div>
                                {{#with button}}
                                    <button type="button" class="btn btn-outline-warning">{{text}}</button>
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                    <h6>{{header_text}}</h6>
                        <div>
                            {{#with button}}
                                <button type="button" class="btn btn-info">{{text}}</button>
                            {{/with}}
                        </div>
                    </div>
                </div>
            {{/with}}
        </div>
    </div>
    <br>    
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
                <p class="text-start text-uppercase">{{title}}</p>
                <p class="text-start text-lowercase">{{text}}</p>
                <div>
                    {{#with button}}
                        <a href="https://google.com" target="_blank" class="btn btn-link">{{text}}</a>
                    {{/with}} 
                    <br><br>           
                <div class="card">
                    <div class="card-body">
                        {{#each list_name}}
                            <li><a href={{url}}  target="_blank" class="btn btn-link">{{text}}</a></li>
                        
                        {{/each}}
                    </div>
                </div>   
                
            {{/with}}
        </div>
    </div>
</Main>